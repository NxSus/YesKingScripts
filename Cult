local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "CultivationGame", HidePremium = false, SaveConfig = true, ConfigFolder = "Config"})

--Values
_G.autoCult = True


--Functions

function autoCult()
  while _G.autoCult == True do 
   wait()
   game:GetService("ReplicatedStorage").UpdateCultivation:FireServer()
 end
end

--Tabs
local Tab = Window:MakeTab({
	Name = "Mabin",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--Buttons


--Toggles
Tab:AddToggle({
	Name = "Auto Cultivate Times 10",
	Default = false,
	Callback = function(Value)
		_G.autoCult = Value
                autoCult()
	end    
})





OrionLib:Init()
