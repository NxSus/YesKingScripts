local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "CultivationGame", HidePremium = false, SaveConfig = true, ConfigFolder = "Config"})

--Values
_G.autoCult = true


--Functions

function autoCult()
 while _G.autoCult == true do 
   game:GetService("ReplicatedStorage").UpdateCultivation:FireServer()
   wait(.0000000000000000000000000001)
  end
 end

--Tabs
local Tab = Window:MakeTab({
	Name = "Mabin",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--Buttons
Tab:AddButton({
	Name = "Cultivate Multiplier",
	Callback = function()
         while true do
          wait() --you need this, otherwise roblox will crash
          game:GetService("ReplicatedStorage").UpdateCultivation:FireServer()
         end
  	end    
})


--Toggles
Tab:AddToggle({
	Name = "Auto Cultivate",
	Default = false,
	Callback = function(Value)
	  _G.autoCult = Value
          autoCult()
	end    
})





OrionLib:Init()
