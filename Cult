local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Orion", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--Values
_G.autoCult = true


--Functions

function autoCult()
 while _G.autoCult == true do 
   local args = {
    [1] = "Air"
}

game:GetService("ReplicatedStorage"):WaitForChild("ComprehendDao"):FireServer(unpack(args))
   wait(.000000000000000000000000001)
  end
 end

function autoPill()
 while _G.autoPill == true do 
   local args = {
    [1] = "Fire"
}

game:GetService("ReplicatedStorage"):WaitForChild("ComprehendDao"):FireServer(unpack(args))
   wait(.000000000000000000000000001)
  end
 end

function autoPillTw()
 while _G.autoPillTw == true do 
   local args = {
    [1] = "Water"
}

game:GetService("ReplicatedStorage"):WaitForChild("ComprehendDao"):FireServer(unpack(args))
   wait(.000000000000000000000000001)
  end
 end

function autoFight()
 while _G.autoFight == true do
   game:GetService("ReplicatedStorage").GenerateFight:FireServer()
   wait(.001)
  end
 end

function Earth()
 while _G.Earth == true do 
   local args = {
    [1] = "Earth"
}

game:GetService("ReplicatedStorage"):WaitForChild("ComprehendDao"):FireServer(unpack(args))
   wait(.000000000000000000000000001)
  end
 end

--Tabs
local Tab = Window:MakeTab({
	Name = "Mabin",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--Buttons
Tab:AddButton({
	Name = "Cultivate Multiplier",
	Callback = function()
         while true do
          wait() --you need this, otherwise roblox will crash
          game:GetService("ReplicatedStorage").UpdateCultivation:FireServer()
         end
  	end    
})



--Toggles
Tab:AddToggle({
	Name = "ComprehendAir",
	Default = false,
	Callback = function(Value)
	  _G.autoCult = Value
          autoCult()
	end    
})

Tab:AddToggle({
	Name = "Auto Fight",
	Default = false,
	Callback = function(Value)
	  _G.autoFight = Value
          autoFight()
	end    
})

Tab:AddToggle({
	Name = "ComprehendFire",
	Default = false,
	Callback = function(Value)
	  _G.autoPill = Value
          autoPill()
	end    
})

Tab:AddToggle({
	Name = "ComprehendWater",
	Default = false,
	Callback = function(Value)
	  _G.autoPillTw = Value
          autoPillTw()
	end    
})

Tab:AddToggle({
	Name = "ComprehendEarth",
	Default = false,
	Callback = function(Value)
	  _G.Earth = Value
          Earth()
	end    
})




OrionLib:Init()
